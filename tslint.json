{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "member-access": [
            false
        ],
        "interface-name": [
            false
        ],
        "curly": false,
        "variable-name": [true, "allow-leading-underscore"],
        "no-string-literal": false,
        "no-namespace": false,
        "object-literal-sort-keys": false,
        "max-classes-per-file": false
    }
}
